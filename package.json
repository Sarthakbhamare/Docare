{
  "name": "docare-health",
  "version": "2.0.0",
  "description": "DoCare Health - Compassionate Healthcare Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint assets/js/**/*.js",
    "lint:fix": "eslint assets/js/**/*.js --fix",
    "format": "prettier --write \"assets/**/*.{js,css,html}\"",
    "format:check": "prettier --check \"assets/**/*.{js,css,html}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "healthcare",
    "telemedicine",
    "health-platform",
    "medical-records",
    "appointments"
  ],
  "author": "DoCare Health Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sarthakbhamare/Docare.git"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "fuse.js": "^7.0.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.0",
    "zustand": "^4.4.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
